match $(host) http://localhost:19999/manager

# /manager/login 管理员登录
# 成功
post $(host)/login
json '{"Name": "e99", "Password": "123456"}'
ret 200
json '{"data": $(token),"error": 0,"msg": "success"}'

# 失败
post $(host)/login
json '{"Name": "e99", "Password": "111111"}'
ret 403
json '{"error":40300,"msg":"账号或密码错误！"}'

# /manager/challenge 添加靶机
post $(host)/challenge
header Authorization $(token)
json '{"Title":"Test Web1", "BaseScore": 1000}'
ret 200
json '{"data":"添加 Challenge 成功！","error":0,"msg":"success"}'

# 重复添加测试
post $(host)/challenge
header Authorization $(token)
json '{"Title":"Test Web1", "BaseScore": 1000}'
ret 403
